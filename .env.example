# Environment settings
NETUID=23                  # Subnet ID
DEBUG=False                 # Set to False by default
SUBTENSOR_NETWORK="finney" # Subtensor network to use

# Bittensor settings
WALLET_PATH=~/.bittensor/wallets  # Default path
WALLET_NAME=custom_wallet  # Override default wallet name
WALLET_HOTKEY=main         # Custom hotkey name

# API Keys
DATURA_API_KEY=your_datura_api_key_here  # Required (no default)
# NINETEEN_API_KEY=your_19_api_key         # Optional (defaults to empty string)

# Database configuration
DATABASE_URL=sqlite+aiosqlite:///./nuance.db
# DATABASE_URL uses this format: sqlite+aiosqlite:///path/to/file.db

# Database connection pool settings
DATABASE_POOL_SIZE=5             # Default
DATABASE_MAX_OVERFLOW=10         # Default
DATABASE_POOL_TIMEOUT=30         # Default (seconds)
DATABASE_ECHO=True               # Set to True for debugging

# Submission server, setup this to get direct submissions from miners and gossip from other validators
SUBMISSION_SERVER_HOST="0.0.0.0"    # Default
# This port need to be set explicitly, must be external port if your machine is behind mapping or NAT
# SUBMISSION_SERVER_PORT=10000